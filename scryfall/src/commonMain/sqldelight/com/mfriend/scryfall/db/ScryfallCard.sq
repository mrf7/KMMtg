CREATE TABLE CachedCard(
    name TEXT NOT NULL,
    set_code TEXT NOT NULL,
    set_name TEXT NOT NULL,
    image_url TEXT,
    scryfall_url TEXT NOT NULL,
    api_url TEXT NOT NULL,
    image_uri TEXT NOT NULL,
    id TEXT NOT NULL
);

insert:
INSERT INTO CachedCard(name,set_code, set_name,image_url,scryfall_url, api_url, image_uri, id)
VALUES ?;

searchByName:
SELECT * FROM CachedCard WHERE name LIKE ('%' || :term || '%');